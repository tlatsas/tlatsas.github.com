---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>tasos at github</title>
    <link href="http://tlatsas.github.com/atom.xml" rel="self"/>
    <link href="http://tlatsas.github.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://tlatsas.github.com/</id>
    <author>
        <name>Tasos Latsas</name>
        <email>tlatsas@users.sf.net</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://tlatsas.github.com/{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://tlatsas.github.com/{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
