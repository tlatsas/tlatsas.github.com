---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>{{ site.title }}</title>
    <link href="http://www.kodama.gr/atom.xml" rel="self"/>
    <link href="http://www.kodama.gr"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://www.kodama.gr/</id>
    <author>
        <name>Tasos Latsas</name>
        <email>tlatsas@users.sf.net</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://www.kodama.gr{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://www.kodama.gr{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
